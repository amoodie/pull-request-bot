      "followers_url": "https://api.github.com/users/amoodie/followers",
      "following_url": "https://api.github.com/users/amoodie/following{/other_user}",
      "gists_url": "https://api.github.com/users/amoodie/gists{/gist_id}",
      "starred_url": "https://api.github.com/users/amoodie/starred{/owner}{/repo}",
